###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#VisitingMall

:VisitingMall rdf:type activities:ComplexOneOfActivity ,
                       owl:NamedIndividual ;
              
              activities:hasPart :Visiting_mall_A ,
                                 :Visiting_mall_B .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#Visiting_mall_A

:Visiting_mall_A rdf:type activities:ShoppingActivity ,
                          owl:NamedIndividual ;
                 
                 activities:hasTemporalWindow :mall_A_open_hours .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#Visiting_mall_B

:Visiting_mall_B rdf:type activities:ShoppingActivity ,
                          owl:NamedIndividual ;
                 
                 activities:hasTemporalWindow :mall_B_open_hours .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#instant_A_end

:instant_A_end rdf:type owl:NamedIndividual ,
                        time:Instant ;
               
               time:inXSDDateTime "2014-12-06T09:00:00"^^xsd:dateTime .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#instant_A_start

:instant_A_start rdf:type owl:NamedIndividual ,
                          time:Instant ;
                 
                 time:inXSDDateTime "2014-12-06T15:00:00"^^xsd:dateTime .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#instant_B_end

:instant_B_end rdf:type owl:NamedIndividual ,
                        time:Instant ;
               
               time:inXSDDateTime "2014-12-06T12:00:00"^^xsd:dateTime .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#instant_B_start

:instant_B_start rdf:type owl:NamedIndividual ,
                          time:Instant ;
                 
                 time:inXSDDateTime "2014-12-06T20:00:00"^^xsd:dateTime .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#mall_A_open_hours

:mall_A_open_hours rdf:type activities:TimeInterval ,
                            owl:NamedIndividual ;
                   
                   activities:hasEndTime :instant_A_end ;
                   
                   activities:hasStartTime :instant_A_start .



###  http://www.uom.gr/ai/ontologies/ActivitiesExamples.owl#mall_B_open_hours

:mall_B_open_hours rdf:type activities:TimeInterval ,
                            owl:NamedIndividual ;
                   
                   activities:hasEndTime :instant_B_end ;
                   
                   activities:hasStartTime :instant_B_start .




