###  http://www.uom.gr/ai/ontologies/Activities.owl#CommunalActivity

:CommunalActivity rdf:type owl:Class ;
                  
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:intersectionOf ( :HumanActivity
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasOtherParticipant ;
                                                               owl:someValuesFrom :Participant
                                                             ]
                                                           )
                                      ] ;
                  
                  rdfs:subClassOf :HumanActivity .



###  http://www.uom.gr/ai/ontologies/Activities.owl#ComplexActivity

:ComplexActivity rdf:type owl:Class ;
                 
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasPart ;
                                                       owl:someValuesFrom :Activity
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasPart ;
                                                       owl:someValuesFrom :ComplexActivity
                                                     ]
                                                   )
                                     ] ;
                 
                 owl:disjointUnionOf ( :ComplexDisjointActivity
                                       :ComplexUnionActivity
                                     ) .



###  http://www.uom.gr/ai/ontologies/Activities.owl#ComplexDisjointActivity

:ComplexDisjointActivity rdf:type owl:Class ;
                         
                         rdfs:subClassOf :ComplexActivity .



