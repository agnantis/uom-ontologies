@prefix : <http://www.uom.gr/ai/ontologies/Activities.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.uom.gr/ai/ontologies/Activities.owl> .

<http://www.uom.gr/ai/ontologies/Activities.owl> rdf:type owl:Ontology ;
                                                 
                                                 owl:versionIRI <http://www.uom.gr/ai/ontologies/Activities.owl> ;
                                                 
                                                 owl:imports <http://www.w3.org/2006/time> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.uom.gr/ai/ontologies/Activities.owl#hasDuration

:hasDuration rdf:type owl:ObjectProperty ;
             
             rdfs:range <http://www.w3.org/2006/time#DurationDescription> .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasEndTime

:hasEndTime rdf:type owl:FunctionalProperty ,
                     owl:ObjectProperty ;
            
            rdfs:range <http://www.w3.org/2006/time#Instant> .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasLocation

:hasLocation rdf:type owl:ObjectProperty ;
             
             rdfs:range :GeoLocation ;
             
             rdfs:domain :Venue .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasOtherParticipant

:hasOtherParticipant rdf:type owl:ObjectProperty .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasPart

:hasPart rdf:type owl:ObjectProperty .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasStartTime

:hasStartTime rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:range <http://www.w3.org/2006/time#Instant> .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasTemporalWindow

:hasTemporalWindow rdf:type owl:ObjectProperty .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasVenue

:hasVenue rdf:type owl:ObjectProperty ;
          
          rdfs:range :Venue .



###  http://www.uom.gr/ai/ontologies/Activities.owl#requires

:requires rdf:type owl:ObjectProperty ;
          
          rdfs:comment "Μια δραστηριότητα πιθανώς να απαιτεί ένα ή περισσότερα πραγματα. Για παράδειγμα, η παρακολούθηση ενός αγώνα μπάσκετ, μπορεί να απαιτεί να έχουμε μαζί μας ένα εισιτήριο"@el ,
                       "An activity may require one or more things. Fox example, watching a basketball game may require to have a ticket with you"@en .



###  http://www.uom.gr/ai/ontologies/Activities.owl#transportationTypeFromVenue

:transportationTypeFromVenue rdf:type owl:ObjectProperty ;
                             
                             rdfs:range :Transportation .



###  http://www.uom.gr/ai/ontologies/Activities.owl#transportationTypeToVenue

:transportationTypeToVenue rdf:type owl:ObjectProperty ;
                           
                           rdfs:range :Transportation .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.uom.gr/ai/ontologies/Activities.owl#hasLatitude

:hasLatitude rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             
             rdfs:range xsd:double .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasLongitude

:hasLongitude rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:range xsd:double .



###  http://www.uom.gr/ai/ontologies/Activities.owl#hasName

:hasName rdf:type owl:DatatypeProperty ;
         
         rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.uom.gr/ai/ontologies/Activities.owl#ActiveActivity

:ActiveActivity rdf:type owl:Class ;
                
                rdfs:subClassOf :HumanActivity ;
                
                owl:disjointWith :PassiveActivity .



###  http://www.uom.gr/ai/ontologies/Activities.owl#ActivitiesExample

:ActivitiesExample rdf:type owl:Class .



###  http://www.uom.gr/ai/ontologies/Activities.owl#Activity

:Activity rdf:type owl:Class ;
          
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasVenue ;
                            owl:someValuesFrom owl:Thing
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDuration ;
                            owl:someValuesFrom <http://www.w3.org/2006/time#DurationDescription>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :transportationTypeToVenue ;
                            owl:someValuesFrom owl:Thing
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :requires ;
                            owl:someValuesFrom owl:Thing
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTemporalWindow ;
                            owl:someValuesFrom :TimeInterval
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :transportationTypeFromVenue ;
                            owl:someValuesFrom owl:Thing
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasStartTime ;
                            owl:someValuesFrom <http://www.w3.org/2006/time#Instant>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasEndTime ;
                            owl:someValuesFrom <http://www.w3.org/2006/time#Instant>
                          ] .



